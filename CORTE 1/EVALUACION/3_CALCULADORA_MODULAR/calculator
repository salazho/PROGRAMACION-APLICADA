class calculator:
    def suma(self, a, b):
        return a + b

    def resta(self, a, b):
        return a - b

    def multi(self, a, b):
        return a * b

    def division(self, a, b):
        try:
            return a / b
        except ZeroDivisionError:
            return "ERROR: Division por cero no permitida"
        
    def modulo(self, a, b):
        try:
            return a % b
        except ZeroDivisionError:
            return "ERROR: Division por cero no permitida"
